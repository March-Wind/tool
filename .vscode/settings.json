{
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript"
    ],
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "editor.tabSize": 2,
    "eslint.options": {
        "overrideConfigFile": ".eslintrc.js"
    }
}