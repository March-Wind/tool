    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-typescript": "^1.0.1",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-prettier": "^2.0.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.8.1",
    "tsc-alias": "^1.6.9",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "ajax-hook": "^2.0.3",
    "axios": "^0.24.0",
