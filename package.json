    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.2.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.4.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.0",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.45",
