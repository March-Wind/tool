{
  "name": "tool",
  "version": "1.0.0",
  "description": "",
  "main": "./lib/index.js",
  "scripts": {
    "dev:web": "cross-env BUILD_TYPE=dev:web rollup --c config/rollup.webserver.js -w",
    "dev:node": "TS_NODE_PROJECT=./tsconfig.json node --loader ts-node/esm  ./src/test.ts-node.ts",
    "dev:node:debug": "TS_NODE_PROJECT=./tsconfig.json node --inspect-brk  --loader ts-node/esm  ./src/test.ts-node.ts",
    "build:sdk": "npm run tsc && cross-env BUILD_TYPE=sdk rollup --config ./config/rollup.sdk.js",
    "tsc": "tsc && tsc-alias",
    "rollup": "rollup --config ./config/rollup.config.js",
    "prepublish": "npm run build:sdk",
    "eslint": "eslint ./src/**/* --fix",
    "test": "jest --passWithNoTests",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand  __tests__/javascript-language/promise/concurrency.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
